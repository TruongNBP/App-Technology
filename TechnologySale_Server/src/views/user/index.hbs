<body>

  <div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">User Management</h4>
        <p class="card-description">
        </p>
        <div class="table-responsive">
          <table class="table table-hover ">
            <thead>
              <tr>
                <th>Index</th>
                <th>Name</th>
                <th>Age</th>
                <th>Email</th>
                <th>Phone</th>


              </tr>
            </thead>
            <tbody>
              {{#each user}}
              <tr class="odd gradeX">
                <td>{{this.index}}</td>

                <td>{{this.name}}</td>
                <td>{{this.age}}</td>
                <td>{{this.email}}</td>
                <td>{{this.phone}}</td>


              </tr>
              {{/each}}
            </tbody>

          </table>
        </div>
      </div>
    </div>
  </div>
</body>